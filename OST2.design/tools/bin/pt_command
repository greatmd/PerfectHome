# Script to run  Primetime for OpenSPARC T2

set block = spc;
set blk_dir = ${DV_ROOT};
set blk_dir = "$blk_dir/design/sys/iop/$block/synopsys";
echo "PNR target module" $block;
echo "Enter $blk_dir"; 
cd $blk_dir; 
pt_shell -f $DV_ROOT/design/sys/synopsys/script/pt_spc.tcl | tee  pt_shell.log
